{
    "manifest_version" : 2,

    "name" : "webstagram",
    "description" : "This extension adds an instagram inspired filter to web sites",
    "version" : "1.0",

    "permissions" : [
        "tabs", "http://*/*", "https://*/*", "storage"
    ],

    "browser_action" : {
        "default_popup" : "popup.html"
    }

}